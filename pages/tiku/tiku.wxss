
.tiku-head{
	display: flex;
	justify-content: space-between;
	padding-top: 26rpx;
	background: #fff;
}
.tiku-head-l .van-search{
	height:68rpx
}
.tiku-head-l .van-search .van-search__content{
	border-radius: 34rpx;
	width: 458rpx;
}
.tiku-head-r{
	width:220rpx;
	padding-right: 16rpx;
}
.tiku-head-r-text{
	font-size: 24rpx;
	color:#333;
}
.tiku-head-r-date{
	font-size: 24rpx;
}
.tiku-head-r-date-text{
	padding:4rpx 10rpx;
	margin: 0rpx 5rpx;
	background-color: #f7f7f7;
	color: #FF7944;
	font-weight: bold;
}
.tiku-body{
	background-color: #fff;
	padding:44rpx 32rpx;
}
.tiku-part1-title{
	margin-bottom: 40rpx;
}
.tiku-part1-con{
	padding:40rpx 0rpx 0rpx 40rpx;
	border-radius: 8rpx;
	position: relative;
}
.tiku-part1-con .van-icon{
	font-size: 32rpx;
position: absolute;
top: 50%;
right: 20rpx;
margin-top: -16rpx;
color: #666;
}
.tiku-part1-con-chart{
	width:200rpx;
	height:200rpx;
	flex: 1;
text-align: center;
}
.tiku-part1-con{
	width: 330rpx;
	height:156rpx;
	background-color: #f7f7f7;
	margin-left: 42rpx;
}
.tiku-part2{
	margin-top: 60rpx;
}
.tiku-part2-list{
	display: flex;
	justify-content: space-between;
	height: 168rpx;
	padding: 0rpx 40rpx;
	margin-bottom:20rpx;
	position: relative;
}
.tiku-part2-list image{
	width: 100%;
	height: 100%;
	position: absolute;
	left: 0rpx;
	top: 0rpx;
}
.tiku-part2-list-l{
	font-size: 32rpx;
	line-height: 168rpx;
	z-index: 1;
}
.tiku-part2-list-r{
	margin-top: 36rpx;
	z-index: 1;
}
.tiku-part2-list-r-t{
	margin-bottom: 6rpx;
}
.tiku-part2-list-r-t-text{
	font-size: 28rpx;
}
.tiku-part2-list-r-t-num{
	font-size: 40rpx;
}
.tiku-part2-list-r-b{
	font-size: 24rpx;
	color:#9c9c9e;
}
.tiku-part3{
	display: flex;
}
.tiku-part3-icon{
	width: 84rpx;
	height: 60rpx;
	margin-right: 20rpx;
}
.tiku-part3-txt{
	font-size: 24rpx;
	color:#666;
}